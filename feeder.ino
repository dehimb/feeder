#define SCREEN_WIDTH 128
#define SCREEN_HEIGHT 64

#define OLED_RESET -1  // Reset pin
#define SCREEN_ADDRESS 0x3C
Adafruit_SSD1306 display(SCREEN_WIDTH, SCREEN_HEIGHT, &Wire, OLED_RESET);

#define LEDR D3
#define LEDG D4
#define BTN D8
#define SW D7

int currentFrame = 0;
bool active = false;
int lastButtonState = 0;
int lastKnobButtonState = 1;
long lastFrameChange = millis();

long encoderPosition  = 0;
Encoder myEnc(D5, D6);

Servo servo;
bool isFeederOpen = false;
long timer = 0;
// Encoder rotatation step in millis
long timerEncoderStep = 1000 * 60 * 10;
long timerStart = 0;

long hour = 3600000; // 3600000 milliseconds in an hour
long minute = 60000; // 60000 milliseconds in a minute
long second =  1000; // 1000 milliseconds in a second

void setup() {
  Serial.begin(9600);
  display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS);

  // Status leds
  pinMode(LEDR, OUTPUT);
  pinMode(LEDG, OUTPUT);
  // Status button
  pinMode(BTN, INPUT);
  // Knob button
  pinMode(SW, INPUT_PULLUP);

  digitalWrite(LEDR, LOW);
  digitalWrite(LEDG, LOW);

  servo.attach(D0);
  // In case of reboot or power issues open feeder by default
  setFeederOpen(true);
}

void loop() {
  redraw();
  if (!active) {
    digitalWrite(LEDR, HIGH);
    digitalWrite(LEDG, LOW);
  } else {
    digitalWrite(LEDR, LOW);
    digitalWrite(LEDG, HIGH);
  }

  // Habdle status button click
  int buttonState = digitalRead(BTN);
  if (buttonState != lastButtonState) {
    if (buttonState == HIGH) {
      if (encoderPosition > 0) {
        toggleState();
      } else {
        // Ability to toggle feeder while timer is not set
        setFeederOpen(!isFeederOpen);
      }
    }
  }
  lastButtonState = buttonState;

  // Handle knob button click
  int knobButtonState = digitalRead(SW);
  if (knobButtonState != lastKnobButtonState) {
    if (knobButtonState == LOW) {
      setFeederOpen(!isFeederOpen);
    }
  }
  lastKnobButtonState = knobButtonState;

  // No more that 9 hours timer
  if (timer / hour > 9) {
    myEnc.readAndReset();
  }

  // Handle encoder rotation
  long newEncoderPosition = myEnc.read();
  if (newEncoderPosition != encoderPosition && newEncoderPosition % 2 == 0 && !active) {
    if (newEncoderPosition > 0) {
      myEnc.readAndReset();
    } else {
      encoderPosition = newEncoderPosition * -1 / 2;
      // Close feeder when setting the timer
      if (isFeederOpen) {
        setFeederOpen(false);
      }
    }
  }

  // Calculate timer
  if (active) {
    timer = encoderPosition * timerEncoderStep + timerStart - millis();
    if (timer < 0) {
      // Time to feed
      toggleState();
      setFeederOpen(true);
    }
  } else {
    timer = encoderPosition * timerEncoderStep;
  }
}

void setFeederOpen(bool open) {
  if (open) {
    servo.write(0);
    isFeederOpen = true;
  } else {
    servo.write(199);
    isFeederOpen = false;
  }
}

// Prepare nice string for display
String getLeftTimeString() {
  String hoursStr = String(timer / hour);
  int minutes = (timer % hour) / minute;
  String minutesStr = String(minutes);
  if (minutes < 10) {
    minutesStr = "0" + minutesStr;
  }
  int seconds = ((timer % hour) % minute) / second;
  String secondsStr = String(seconds);
  if (seconds < 10) {
    secondsStr = "0" + secondsStr;
  }
  return hoursStr + ":" + minutesStr + ":" + secondsStr;
}

void toggleState() {
  active = !active;
  if (active) {
    timerStart = millis();
  } else {
    encoderPosition = 0;
    myEnc.readAndReset();
  }
}

void redraw() {
  display.clearDisplay();
  // Draw timer
  display.setTextSize(3);
  display.setCursor(2, 0);
  display.setTextColor(WHITE);
  display.println(getLeftTimeString());
  if (millis() - lastFrameChange > 50) {
    currentFrame++;
    lastFrameChange = millis();
  }
  // Draw bitmap
  if (currentFrame > 11 || !active) {
    // Cycle bitmaps for active timer and keep static for non-active state
    currentFrame = 0;
  }
  display.drawBitmap(5, 24, frames[currentFrame], 128, 40, WHITE);
  display.display();
}
